<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dataProvider="clr-namespace:ML.Guide.DataProvider">


    <ObjectDataProvider MethodName="GetGDModelTypes"
                        ObjectType="{x:Type dataProvider:DataProvider}"
                        x:Key="GDModelTypes"/>

    <ObjectDataProvider MethodName="GetOptimizerTypes"
                        ObjectType="{x:Type dataProvider:DataProvider}"
                        x:Key="OptimizerTypes"/>

    <ObjectDataProvider MethodName="GetLossTypes"
                        ObjectType="{x:Type dataProvider:DataProvider}"
                        x:Key="LossTypes"/>

    <ObjectDataProvider MethodName="GetMerticTypes"
                        ObjectType="{x:Type dataProvider:DataProvider}"
                        x:Key="MetricTypes"/>

</ResourceDictionary>